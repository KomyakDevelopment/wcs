{
  "name": "WCS",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "Client": {
        "$className": "Folder",
        "controllers": {
          "$path": "src/client/controllers"
        },
        "managers": {
          "$path": "src/client/managers"
        },
        "ui": {
          "$path": "src/client/ui"
        }
      },
      "Packages": {
        "$path": "Packages"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "$properties": {
        "EnableMouseLockOption": false,
        "DevComputerMovementMode": "UserChoice"
      },
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    }
  }
}