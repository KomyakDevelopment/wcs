{
  "globIgnorePaths": [
    "**/package.json",
    "**/tsconfig.json"
  ],
  "name": "wcs",
  "tree": {
    "$path": "out",
    "include": {
      "$path": "include",
      "node_modules": {
        "$className": "Folder",
        "@flamework": {
          "$path": "node_modules/@flamework"
        },
        "@rbxts": {
          "$path": "node_modules/@rbxts"
        }
      }
    }
  }
}